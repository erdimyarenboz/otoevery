{
    "version": 2,
    "buildCommand": "npx prisma generate",
    "outputDirectory": ".",
    "installCommand": "npm install",
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/api"
        }
    ],
    "functions": {
        "api/index.ts": {
            "maxDuration": 30
        }
    }
}